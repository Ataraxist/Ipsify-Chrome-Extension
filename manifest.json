{
  "manifest_version": 3,
  "name": "Image Scrape",
  "version": "1.0",
  "description": "Click Button to get images",
  "permissions": ["scripting", "downloads", "activeTab", "notifications"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Scraper",
    "default_icon": {
      "16": "enabled16.png",
      "48": "enabled48.png",
      "128": "enabled128.png"
    }
  },
  "icons": {
    "16": "enabled16.png",
    "48": "enabled48.png",
    "128": "enabled128.png"
  }
}
