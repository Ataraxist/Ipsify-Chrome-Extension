{
  "manifest_version": 3,
  "name": "Ipsify",
  "version": "1.0",
  "description": "Have you ever wanted to make a webpage totally illegible? Me neither, but now you can!",
  "permissions": ["scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["replacer.js"]
    }
  ],
  "action": {
    "default_title": "Ipsify"
  }
}
